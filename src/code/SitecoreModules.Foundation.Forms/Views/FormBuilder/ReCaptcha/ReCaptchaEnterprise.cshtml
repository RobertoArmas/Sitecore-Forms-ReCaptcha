@using System.Web.Mvc.Html
@using Sitecore.Mvc
@using SitecoreModules.Foundation.Forms.Helpers
@model SitecoreModules.Foundation.Forms.Models.Fields.ReCaptchaEnterpiseViewModel
@{
    var fieldId = Html.IdFor(m => Model.CaptchaValue);
    string recaptchaApiUrl = Sitecore.Configuration.Settings.GetSetting("Foundation.Forms.ReCaptchaEnterprise.Api.Url", "");
    string recaptchaPublicKey;
    if (Model.RecaptchaModeInvisible)
    {
        recaptchaPublicKey = Model.CaptchaPublicKeyV3;
    }
    else
    {
        recaptchaPublicKey = Model.CaptchaPublicKeyV2;
    }
    recaptchaApiUrl = string.Format(recaptchaApiUrl, recaptchaPublicKey);
}

<div class="form-input">
    <div id="@(fieldId)_wrapper"></div>
    <input id="@fieldId" name="@Html.NameFor(m => Model.CaptchaValue)" type="hidden" class="fxt-captcha-enterprise" />
    @Html.ValidationMessageFor(m => Model.CaptchaValue)

    @if (Html.Sitecore().IsExperienceFormsEditMode())
    {
        if (Model.RecaptchaModeInvisible)
        {
            <img width="300px" alt="recaptcha" src="data:image/png;base64," />
        }
        else
        {
            <img alt="recaptcha" src="data:image/png;base64," />
        }
    }
    else
    {
        if (Model.RecaptchaModeInvisible)
        {
            <script src="@recaptchaApiUrl&hl=@Sitecore.Context.Language.CultureInfo.TwoLetterISOLanguageName"></script>
            <script type="text/javascript">
                if (window.siplastRecaptcha == undefined) {
                    window.siplastRecaptcha = {
                        publicKey: "@recaptchaPublicKey",
                        inputs: []
                    }
                }
                document.addEventListener("DOMContentLoaded", (event) => {
                    window.siplastRecaptcha.inputs.push(
                        {
                            action: "homepage",
                            input: window.document.getElementById('@fieldId')
                        }
                    );
                    grecaptcha.enterprise.ready(function() {
                        grecaptcha.enterprise.execute("@recaptchaPublicKey", { action: 'homepage' }).then(function(token) {
                            window.document.getElementById('@fieldId').value = token;
                        });
                    });
                });

            </script>
        }
        else
        {

            <script type="text/javascript">
                var reCaptchaArray = reCaptchaArray || [];
                reCaptchaArray.push(
                    function() {
                        var _captchaInput = window.document.getElementById('@fieldId');
                        window.grecaptcha.enterprise.render('@(fieldId)_wrapper',
                            {
                                'sitekey': '@recaptchaPublicKey',
                                'callback': function(token) {
                                    _captchaInput.value = token;
                                },
                                'expired-callback': function(args) {
                                    _captchaInput.value = '';
                                }
                            });
                    }
                );
            </script>
            <script src="https://www.google.com/recaptcha/enterprise.js?onload=loadReCaptchas&render=explicit&hl=@Sitecore.Context.Language.CultureInfo.TwoLetterISOLanguageName" async defer></script>
        }

    }
</div>